vsim work.toplevel
# vsim work.toplevel 
# Start time: 11:08:30 on Sep 12,2024
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.toplevel(toplevel_arch)
add wave -position insertpoint  \
sim:/toplevel/A \
sim:/toplevel/B \
sim:/toplevel/C \
sim:/toplevel/D \
sim:/toplevel/E \
sim:/toplevel/Y
force -freeze sim:/toplevel/A 0 0, 1 {0 ps} -r 1
force -freeze sim:/toplevel/B 0 0, 1 {0 ps} -r 2
force -freeze sim:/toplevel/C 0 0, 1 {0 ps} -r 4
force -freeze sim:/toplevel/D 0 0, 1 {0 ps} -r 8
force -freeze sim:/toplevel/E 0 0, 1 {2 ps} -r 16
run
run
force -freeze sim:/toplevel/D 0 0, 1 {0 ps} -r 8
restart
run
run
restart
force -freeze sim:/toplevel/A 0 0, 1 {50 ps} -r 100
force -freeze sim:/toplevel/B 0 0, 1 {100 ps} -r 200
force -freeze sim:/toplevel/C 1 0, 0 {200 ps} -r 400
force -freeze sim:/toplevel/C 0 0, 1 {200 ps} -r 400
force -freeze sim:/toplevel/D 0 0, 1 {400 ps} -r 800
force -freeze sim:/toplevel/E 0 0, 1 {800 ps} -r 1600
run
run
run
